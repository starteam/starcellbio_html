{template .display_slide_types}
    {if length(keys($cell_treatment.treatment_list.list[0].conditions)) == 1}

        {foreach $k in keys($cell_treatment.treatment_list.list[0].conditions)}
            <span
                class="scb_f_microscopy_select_slide_type {if $row.is_sample_enabled}{else}scb_s_microscopy_dim_conditions{/if}"
                cell_treatment_id='{$cell_treatment.id}'
                microscopy_id='{$microscopy.id}'
                assignment_id="{$assignment.id}"
                experiment_id="{$experiment.id}"
                value="{$k}"
                lane_kind="{$row.kind}"
                lane_id="{if $row.kind == 'existing'}{$row.lane.id}{/if}"
            >
                {$kinds[$k].name}
            </span>
        {/foreach}
    {else}
	    <select class="scb_f_microscopy_select_slide_type"
	        cell_treatment_id='{$cell_treatment.id}' role='select'
	        microscopy_id='{$microscopy.id}'
	        assignment_id="{$assignment.id}"
	        experiment_id="{$experiment.id}"
	        lane_kind="{$row.kind}"
	        lane_id="{if $row.kind == 'existing'}{$row.lane.id}{/if}"
	        {if $row.is_sample_enabled}{else}disabled="disabled"{/if}
	    >
	        {if $row.kind == 'existing'}
	            {foreach $k in keys($row.cell_treatment.treatment_list.first.conditions)}
	                <option value='{$k}' role='option'{if $row.lane.kind == $k}selected="selected"{/if}>
	                    {$kinds[$k].name}
	                </option>
	            {/foreach}
	        {else}
	            <!--row not existing, want to show Select Analysis option-->
	            <option  role='option' selected="selected" disabled="disabled" value=''>Select Analysis</option>

	            {foreach $k in keys($row.cell_treatment.treatment_list.first.conditions)}
	            <option role='option' value='{$k}'>{$kinds[$k].name}</option>
	            {/foreach}
	        {/if}
	     </select>
{/if}

{/template}



